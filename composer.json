{
    "name": "cnizzardini/cakephp-swagger-bake-demo",
    "description": "Demo Application for Swagger Bake",
    "homepage": "https://cakephpswaggerbake.cnizz.com/",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-simplexml": "*",
        "cakephp/authentication": "^3.x-dev",
        "cakephp/cakephp": "5.x-dev",
        "cakephp/plugin-installer": "^1.2",
        "cnizzardini/cakephp-swagger-bake": "@dev",
        "friendsofcake/search": "dev-cake-5",
        "mobiledetect/mobiledetectlib": "^2.8"
    },
    "require-dev": {
        "cakephp/bake": "@dev",
        "cakephp/debug_kit": "5.x-dev",
        "josegonzalez/dotenv": "^3.2",
        "phpunit/phpunit": "^8.5",
        "psy/psysh": "@stable",
        "cakephp/migrations": "4.x-dev"
    },
    "suggest": {
        "markstory/asset_compress": "An asset compression plugin which provides file concatenation and a flexible filter system for preprocessing and minification.",
        "dereuromark/cakephp-ide-helper": "After baking your code, this keeps your annotations in sync with the code evolving from there on for maximum IDE and PHPStan/Psalm compatibility.",
        "phpstan/phpstan": "PHPStan focuses on finding errors in your code without actually running it. It catches whole classes of bugs even before you write tests for the code."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Demo\\": "plugins/Demo/src/",
            "OtherDemo\\": "plugins/OtherDemo/src/",
            "OtherOpenApi\\": "plugins/OtherOpenApi/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "Demo\\Test\\": "plugins/Demo/tests/",
            "OtherDemo\\Test\\": "plugins/OtherDemo/tests/",
            "OtherOpenApi\\Test\\": "plugins/OtherOpenApi/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-create-project-cmd": "App\\Console\\Installer::postInstall",
        "check": [
            "@test",
            "@cs-check"
        ],
        "cs-check": "phpcs --colors -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "cs-fix": "phpcbf --colors --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "stan": "phpstan analyse src/",
        "test": "phpunit --colors=always"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    },
    "support": {
        "issues": "https://github.com/cnizzardini/cakephp-swagger-bake/issues",
        "wiki": "https://github.com/cnizzardini/cakephp-swagger-bake/wiki",
        "source": "https://github.com/cnizzardini/cakephp-swagger-bake"
    },
    "authors": [
        {
            "name": "Chris Nizzardini",
            "homepage": "https://cakephpswaggerbake.cnizz.com/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "../cakephp-swagger-bake",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "git@github.com:cnizzardini/core.git"
        }
    ]
}
